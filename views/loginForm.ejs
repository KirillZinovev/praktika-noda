      <input type="checkbox" id="chk" aria-hidden="true" />

      <div class="signup">
        <form method="post" action="/login" name="user">
          <label for="chk" aria-hidden="true">Авторизация</label>
          <input
            type="email"
            name="loginForm[email]"
            placeholder="Email"
            required=""
          />
          <input
            type="password"
            name="loginForm[password]"
            placeholder="Пароль"
            required=""
          />

          <button>Войти</button>
        </form>
      </div>
    </div>

